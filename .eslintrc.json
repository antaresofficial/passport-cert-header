{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "plugins": ["unused-imports"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-new": "off",
      "no-underscore-dangle": "off",
      "no-unused-vars": "off",
      "unused-imports/no-unused-imports": "error",
      "unused-imports/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ],
      "sort-imports": [
          "error",
          {
            "ignoreDeclarationSort": true
          }
      ],
      "no-console": "off",
      "import/extensions": "off",
      "func-names": "off",
      "no-multi-assign":"off"
    }
}
